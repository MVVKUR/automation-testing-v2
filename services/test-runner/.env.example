# Server configuration
PORT=8082
NODE_ENV=development

# Redis configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# S3/MinIO configuration
S3_ENDPOINT=http://localhost:9000
S3_REGION=us-east-1
S3_BUCKET=test-artifacts
S3_ACCESS_KEY_ID=minioadmin
S3_SECRET_ACCESS_KEY=minioadmin

# Cypress configuration
CYPRESS_BASE_URL=http://localhost:3000
CYPRESS_SPECS_DIR=./cypress/e2e
CYPRESS_VIDEOS_DIR=./cypress/videos
CYPRESS_SCREENSHOTS_DIR=./cypress/screenshots
CYPRESS_DEFAULT_TIMEOUT=10000
CYPRESS_DEFAULT_BROWSER=electron

# Queue configuration
QUEUE_NAME=test-runner
QUEUE_CONCURRENCY=2
QUEUE_MAX_RETRIES=3

# CORS configuration
CORS_ORIGIN=http://localhost:3000

# API configuration (for test targets)
API_URL=http://localhost:8080
